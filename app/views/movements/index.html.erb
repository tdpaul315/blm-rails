<div class="container">
  <% if user_signed_in? %>
        <h5 style="color:black;text-align:left"> Hi <%= current_user.name.titleize%>!</h5><br />
  <%end%>
</div>

<h3> Check out our Movements Below! </h3>

    <ul>
        <% @movements.each do |m| %>
            <li><%= link_to m.name, m %></li>
                <p>Year Started:<%= m.yr_started %></p>
        <% end %>
    </ul>

    <h4> Popular Movements! </h4>
    <% Movement.more_than_five_protests.each do |p| %>
        <li><%= link_to p.name, p %></li>
    <% end %>