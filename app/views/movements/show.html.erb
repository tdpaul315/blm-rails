

<body class="w3-container">
    
    <h4><b><%= @movement.name %></b></h4>
        <blockquote><%= @movement.description %></blockquote>
            <% @movement.protests.each do |p| %>
               <p><%=link_to p.name, protest_path(p) %></p>
        <% end %>

    <iframe src="https://giphy.com/embed/3ohjVabojNBmsqeqg8" width="380" height="260" frameBorder="0" class="giphy-embed" allowFullScreen></iframe><p><a href="https://giphy.com/gifs/heyarnold-hey-arnold-nicksplat-3ohjVabojNBmsqeqg8">via GIPHY</a></p>

    <div class=" w3-row w3-dark-grey w3-padding-64">
        <h6><%= link_to 'Edit Movement', edit_movement_path(@movement) %></h6>
        <h6><%= link_to "Delete Movement", @movement, method: :delete, data:{confirm: "Do you really want to delete me?"} %></h6>
    </div>

</body>




